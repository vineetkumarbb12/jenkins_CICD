pipeline{
    agent any
    
    stages{
        stage("Build"){
            steps{
                sh '''
                #!/bin/bash
                sleep 10
                echo "========executing BUILD ========"
                '''
            }
        }
        stage("Build1"){
            steps{
                sh '''
                #!/bin/bash
                sleep 10
                echo "========executing BUILD1 ========"
                '''
            }
        }
        stage("Test"){
            steps{
                sh """
                #!/bin/bash
                sleep 10
                echo "========executing TEST ========"
                """
            }
        }
        stage("Test1"){
            steps{
                sh '''
                #!/bin/bash
                sleep 10
                echo "========executing TEST1 ========"
                '''
            }
        }
        stage("Deploy"){
            steps{
                sh '''
                #!/bin/bash
                sleep 10
                echo "========executing DEPLOY ========"
                '''
            }
        }
        stage("Deploy1"){
            steps{
                sh """
                #!/bin/bash
                sleep 10
                echo "========executing DEPLOY1 ========"
                """
            }
        }
    }
}